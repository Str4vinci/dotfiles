#!/usr/bin/env bash

# Install brew
/bin/bash -c "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/HEAD/install.sh)"

export PATH="eval \"$(/home/linuxbrew/.linuxbrew/bin/brew shellenv)\":$PATH"

sudo apt update && sudo apt install stow tmux zsh
rm -f ~/.bashrc ~/.vimrc ~/.bash_aliases
stow bash
stow vim
stow tmux
stow zsh

brew install tree duckdb
